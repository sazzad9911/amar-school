{
  "expo": {
    "name": "Amar School",
    "slug": "amar-school",
    "version": "1.0.5",
    "icon": "./assets/newLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Start.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.amar.school",
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "android": {
      "versionCode":5,
      "adaptiveIcon": {
        "foregroundImage": "./assets/newLogo.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.amar.school",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "runtimeVersion": {
        "policy": "sdkVersion"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b02669ff-1ccd-4571-976d-1739ef783b2c"
      }
    },
    "owner": "scientistx_technology",
    "updates": {
      "url": "https://u.expo.dev/b02669ff-1ccd-4571-976d-1739ef783b2c",
      "enabled":true
    }
  }
}
